{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "gutenlayouts/progressbar",
  "version": "0.1.0",
  "title": "Progressbar",
  "category": "gutenlayouts",
  "description": "Add a customizable svg icon to your content.",
  "parent": [
    "gutenlayouts/progressbars"
  ],
  "supports": {
    "anchor": true,
    "html": false,
    "align": [
      "wide",
      "full"
    ],
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "attributes": {
    "blockStyle": {
      "type": "object"
    },
    "progress": {
      "type": "number",
      "default": 50
    },
    "label": {
      "type": "string",
      "default": "Progress Bar"
    },
    "layout": {
      "type": "string",
      "default": "line"
    },
    "paColor": {
      "type": "string"
    },
    "pinColor": {
      "type": "string"
    },
    "labelSize": {
      "type": "number"
    },
    "labelColor": {
      "type": "number"
    },
    "perceColor": {
      "type": "string"
    },
    "perceSize": {
      "type": "number"
    }
  },
  "usesContext": [
    "gutenlayouts/layout"
  ],
  "textdomain": "gutenlayouts",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}