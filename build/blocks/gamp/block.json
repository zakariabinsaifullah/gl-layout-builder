{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "gutenlayouts/gmap",
  "version": "0.1.0",
  "title": "Google Map",
  "category": "gutenlayouts",
  "description": "Add a customizable svg icon to your content.",
  "supports": {
    "anchor": true,
    "html": false,
    "align": [
      "wide",
      "full"
    ],
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "attributes": {
    "blockStyle": {
      "type": "object"
    },
    "address": {
      "type": "string",
      "default": "New York"
    },
    "zoom": {
      "type": "number",
      "default": "10"
    },
    "height": {
      "type": "number"
    },
    "mapBorder": {
      "type": "object",
      "default": {
        "top": "",
        "right": "",
        "bottom": "",
        "left": ""
      }
    },
    "mapRadius": {
      "type": "number"
    }
  },
  "textdomain": "gutenlayouts",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}