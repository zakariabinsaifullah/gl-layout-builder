{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "gutenlayouts/tab",
    "version": "0.1.0",
    "title": "Tab",
    "category": "gutenlayouts",
    "parent": ["gutenlayouts/tabs"],
    "description": "Embed a Google Map by specifying an address.",
    "supports": {
        "anchor": true,
        "html": false,
        "align": ["wide", "full"],
        "spacing": {
            "margin": true,
            "padding": true,
            "__experimentalDefaultControls": {
                "margin": false,
                "padding": false
            }
        },
        "__experimentalBorder": {
            "__experimentalSkipSerialization": false,
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        },
        "shadow": {
            "__experimentalSkipSerialization": false
        }
    },
    "attributes": {
        "blockStyle": {
            "type": "object"
        },
        "tabId": {
            "type": "string"
        },
        "activeTab": {
            "type": "string",
            "default": "1"
        },

        "parentTabId": {
            "type": "string"
        },
        "animation": {
            "type": "string",
            "default": ""
        },
        "resMode": {
            "type": "string",
            "default": "Desktop"
        },
        "address": {
            "type": "string",
            "default": "New York"
        },
        "type": {
            "type": "string",
            "default": "roadmap"
        },
        "zoom": {
            "type": "number",
            "default": 10
        },
        "height": {
            "type": "object",
            "default": {
                "Desktop": 400,
                "Tablet": 300,
                "Mobile": 200
            }
        },
        "mapBorder": {
            "type": "object",
            "default": {
                "top": "",
                "right": "",
                "bottom": "",
                "left": ""
            }
        },
        "mapRadius": {
            "type": "number"
        }

    },
    "textdomain": "gutenlayouts",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}
