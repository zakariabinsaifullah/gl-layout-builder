{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "gutenlayouts/toggle-content",
    "version": "0.1.0",
    "title": "Toggle Section",
    "category": "gutenlayouts",
    "parent": ["gutenlayouts/advanced-toggle"],
    "attributes": {
        "type": {
            "type": "string",
            "default": "left"
        },
        "badgeText": {
            "type": "string",
            "default": ""
        },
        "badgeBgColor": {
            "type": "string",
            "default": "#ff4757"
        },
        "badgeTextColor": {
            "type": "string",
            "default": "#ffffff"
        }
    },
    "usesContext": [ "gutenlayouts/toggleState" ],
    "textdomain": "gl-layout-builder",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}
